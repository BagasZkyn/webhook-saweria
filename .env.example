# Saweria API Configuration
# Tidak perlu webhook secret - Saweria tidak support generate secret
# Sistem menggunakan IP-based rate limiting + data validation untuk security

# Roblox Game Universe ID (optional)
ROBLOX_UNIVERSE_ID=your_roblox_universe_id_here

# Roblox Open Cloud API Key (optional)
ROBLOX_API_KEY=your_roblox_open_cloud_api_key_here

# Anti-spam: Minimum interval between same donor (seconds)
MIN_DONOR_INTERVAL=10

# Anti-spam: Rate limit per minute (per IP)
RATE_LIMIT_PER_MINUTE=30

# Optional: Database URL untuk persistent storage
DATABASE_URL=your_database_url_here

