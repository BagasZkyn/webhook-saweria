{
  "version": 2,
  "builds": [
    {
      "src": "webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "notify.js",
      "use": "@vercel/node"
    },
    {
      "src": "getDonations.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/webhook.js"
    },
    {
      "src": "/api/notify",
      "dest": "/notify.js"
    },
    {
      "src": "/api/donations",
      "dest": "/getDonations.js"
    }
  ],
  "env": {
    "WEBHOOK_SECRET": "@webhook_secret",
    "ROBLOX_UNIVERSE_ID": "@roblox_universe_id"
  }
}
