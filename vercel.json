{
  "version": 2,
  "builds": [
    {
      "src": "webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "notify.js",
      "use": "@vercel/node"
    },
    {
      "src": "getDonations.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/webhook.js"
    },
    {
      "src": "/api/notify",
      "dest": "/notify.js"
    },
    {
      "src": "/api/donations",
      "dest": "/getDonations.js"
    }
  ]
}

